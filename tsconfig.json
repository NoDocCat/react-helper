    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "rootDir": "./src",
    "noImplicitReturns": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES5"
  },
  "exclude": ["node_modules", "dist", "website"]
}
